@model IEnumerable<ManagerSide.Models.SubCategory>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Tạo mới", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.subcategoryName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category.categoryName)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.subcategoryName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category.categoryName)
            </td>
            <td>
                @Html.ActionLink("Sửa", "Edit", new { id=item.subcategoryID }) |
                @Html.ActionLink("Chi tiết", "Details", new { id=item.subcategoryID }) |
                @Html.ActionLink("Xóa", "Delete", new { id=item.subcategoryID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
